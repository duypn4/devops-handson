       "awsvpcConfiguration": {
          "assignPublicIp": "ENABLED",
          "securityGroups": [ "<your alb security group id>" ],
          "subnets": [ "<your subnet-a id >", "your subnet-b id", "your subnet-c id" ]
       }
    },
    "desiredCount": 1
}